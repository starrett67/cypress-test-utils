{
  "name": "@illumidesk/cypress-test-utils",
  "version": "0.0.0-development",
  "description": "Utilities for testing with Cypress",
  "main": "index.js",
  "author": "Ray Gesualdo <rgesualdo@illumidesk.com> (https://illumidesk.com)",
  "license": "MIT",
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest --coverage",
    "cm": "git-cz",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IllumiDesk/cypress-test-utils.git"
  },
  "bugs": {
    "url": "https://github.com/IllumiDesk/cypress-test-utils/issues"
  },
  "homepage": "https://github.com/IllumiDesk/cypress-test-utils#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "imap-simple": "^4.0.0",
    "lodash": "^4.17.4",
    "micro": "^9.1.0",
    "promise-compose": "^1.1.2",
    "shelljs": "^0.8.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^22.0.6",
    "semantic-release": "^11.0.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false
  },
  "release": {
    "analyzeCommits": {
      "releaseRules": [
        {
          "type": "docs",
          "scope": "README",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "chore",
          "release": "patch"
        }
      ]
    }
  }
}
